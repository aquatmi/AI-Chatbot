<aiml version="1.0.1" encoding="UTF-8">


    <!-- Greetings and Politeness -->
    <category>
        <pattern>HELLO</pattern>
        <template>Hello! How are you?</template>
    </category>

    <category>
        <pattern>HELLO *</pattern>
        <template>
            <srai>HELLO</srai>
        </template>
    </category>

    <category>
        <pattern>HI</pattern>
        <template>
            <srai>HELLO</srai>
        </template>
    </category>

    <category>
        <pattern>HI *</pattern>
        <template>
            <srai>HELLO</srai>
        </template>
    </category>

    <category>
        <pattern>HOW ARE YOU</pattern>
        <template>I'm safe and sound thank you.</template>
    </category>

    <category>
        <pattern>FINE</pattern>
        <template>That's good to know. What would you like to discuss?</template>
    </category>

    <category>
        <pattern>* FINE *</pattern>
        <template><srai>FINE</srai>?
        </template>
    </category>

    <category>
        <pattern>FINE *</pattern>
        <template>
            <srai>FINE</srai>
        </template>
    </category>

    <category>
        <pattern>* FINE</pattern>
        <template>
            <srai>FINE</srai>
        </template>
    </category>

    <category>
        <pattern>GOOD</pattern>
        <template>
            <srai>FINE</srai>
        </template>
    </category>

    <category>
        <pattern>GOOD *</pattern>
        <template>
            <srai>FINE</srai>
        </template>
    </category>

    <category>
        <pattern>* GOOD *</pattern>
        <template><srai>FINE</srai>?
        </template>
    </category>

    <category>
        <pattern>* GOOD</pattern>
        <template>
            <srai>FINE</srai>
        </template>
    </category>

    <category>
        <pattern>BYE</pattern>
        <template>Bye! Remember to wear your seatbelt!</template>
    </category>

    <category>
        <pattern>GOODBYE</pattern>
        <template>
            <srai>BYE</srai>
        </template>
    </category>

    <category>
        <pattern>EXIT</pattern>
        <template>
            <srai>BYE</srai>
        </template>
    </category>

    <category>
        <pattern>THANKS</pattern>
        <template>I'm glad I could help!</template>
    </category>

    <category>
        <pattern>THANK YOU</pattern>
        <template>
            <srai>THANKS</srai>
        </template>
    </category>

    <category>
        <pattern>CHEERS</pattern>
        <template>
            <srai>THANKS</srai>
        </template>
    </category>

    <category>
        <pattern>CHEERS *</pattern>
        <template>
            <srai>THANKS</srai>
        </template>
    </category>

    <!-- HELP -->
    <category>
        <pattern>HELP</pattern>
        <template>Just ask me something and i'll help you out! Or say "Identify" if you want me to tell you what a sign is!</template>
    </category>

    <!-- IDENTIFY IMAGE-->
    <category>
        <pattern>IDENTIFY</pattern>
        <template>Ok! Let me see!</template>
    </category>

    <!-- SET QUESTION ANSWERS -->
    <category>
        <pattern>NATIONAL SPEED LIMIT</pattern>
        <template>In the UK, the National Speed Limit is 70 mph.</template>
    </category>

    <category>
        <pattern>* NATIONAL SPEED LIMIT *</pattern>
        <template>
            <srai>NATIONAL SPEED LIMIT</srai>
        </template>
    </category>

    <category>
        <pattern>* NATIONAL SPEED LIMIT</pattern>
        <template>
            <srai>NATIONAL SPEED LIMIT</srai>
        </template>
    </category>

    <category>
        <pattern>NATIONAL SPEED LIMIT *</pattern>
        <template>
            <srai>NATIONAL SPEED LIMIT</srai>
        </template>
    </category>

    <category>
        <pattern>SIDE OF THE ROAD</pattern>
        <template>In the UK, we drive on the Left-Hand Side.</template>
    </category>

    <category>
        <pattern>* SIDE OF THE ROAD</pattern>
        <template>
            <srai>SIDE OF THE ROAD</srai>
        </template>
    </category>

    <category>
        <pattern>SIDE OF THE ROAD *</pattern>
        <template>
            <srai>SIDE OF THE ROAD</srai>
        </template>
    </category>

    <category>
        <pattern>* SIDE OF THE ROAD *</pattern>
        <template>
            <srai>SIDE OF THE ROAD</srai>
        </template>
    </category>


    <!-- FUTURE DEVELOPMENTS -->
    <category>
        <pattern>CAN YOU IDENTIFY THIS ROAD SIGN FOR ME</pattern>
        <template>Not at the moment, but I will be able to eventually!</template>
    </category>

    <category>
        <pattern>CAN YOU ASK ME A QUESTION ABOUT ROAD SAFETY</pattern>
        <template>Ok; When will the Road Safety Quiz be implemented? Answer: Not Yet!</template>
    </category>


    <!-- IF NO SET RESPONSE -->
    <category>
        <pattern>*</pattern>
        <template>
            <star index="1"/>
        </template>
    </category>

</aiml>